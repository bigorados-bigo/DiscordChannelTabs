{
  "manifest_version": 3,
  "name": "Discord Tabs (Experimental)",
  "version": "0.1.0",
  "description": "Adds a minimal tab bar to discord.com/app (experimental preview).",
  "icons": {},
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://discord.com/*"],
      "js": ["storageBridge.js"],
      "run_at": "document_start"
    },
    {
      "matches": ["https://discord.com/*"],
      "js": ["content.js"],
      "css": ["tabBar.css"],
      "run_at": "document_start",
      "world": "MAIN"
    }
  ],
  "host_permissions": ["https://discord.com/*"],
  "action": {
    "default_title": "Discord Tabs"
  }
}
